format PE console
entry start

include 'win32a.inc' 

; ===============================================
section '.text' code readable executable

start:
	call	read_hex
	mov	esi,eax
	mov	edi,0
	mov	ecx,0
	mov	ebx,1
while_1:
	inc	edi
	cmp	edi,esi
	jae	end_while_1
while_2:
	inc	ebx
	cmp	ebx,edi
	jae	end_while_2
	mov     eax,edi
	mov	edx,0
	div	ebx
	cmp	edx,0
	jnz	not_divisible
	inc	ecx
not_divisible:
	jmp	while_2
end_while_2:
	cmp	ecx,2
	jnz	dont_compute
	mov	eax,edi
	call	print_eax	
dont_compute:
	mov	ebx,1
	mov	ecx,0		
	jmp	while_1
end_while_1:
    ; Exit the process:
	push	0
	call	[ExitProcess]

include 'training.inc'

